{
  "name": "2-type",
  "version": "1.0.0",
  "private": true,
  "description": "2 player typing game",
  "repository": "https://github.com/Hackbit/nko2017-yukkuri",
  "license": "UNLICENSED",
  "author": "Team Yukkuri",
  "dependencies": {
    "koa": "^2.4.1",
    "koa-static": "^4.0.1"
  },
  "scripts": {
    "start": "node ./js/server/server.js",
    "build:dev": "./node_modules/.bin/webpack --config webpack.config.js",
    "build:release": "./node_modules/.bin/webpack --config webpack.release.config.js",
    "css": "./node_modules/.bin/node-sass sass/app.sass -o public",
    "css:watch-files": "./node_modules/.bin/node-sass sass/app.sass -wo public",
    "css:watch": "npm run css && npm run css:watch-files"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "node-sass": "^4.6.0",
    "webpack": "^3.8.1",
    "webpack-strip": "^0.1.0"
  }
}
